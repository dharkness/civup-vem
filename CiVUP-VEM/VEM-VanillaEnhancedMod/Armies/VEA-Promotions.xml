<?xml version="1.0" encoding="utf-8"?>
<GameData>
	<UnitPromotions>
		<Update>
			<Where Type="PROMOTION_MEDIC" />
			<Set AdjacentTileHealChange="2"
				 SameTileHealChange="2"
				 />
		</Update>
		<Update>
			<Where Type="PROMOTION_SUPPLY" />
			<Set EnemyHealChange="2"
				 NeutralHealChange="2"
				 />
		</Update>
		<Update>
			<Where Type="PROMOTION_SURVIVALISM_1" />
			<Set EnemyHealChange="2"
				 NeutralHealChange="2"
				 />
		</Update>
		<Update>
			<Where Type="PROMOTION_HEAL_IF_DESTROY_ENEMY" />
			<Set HPHealedIfDestroyEnemy="20"
				 />
		</Update>
		<Update>
			<Where Type="PROMOTION_PARTIAL_HEAL_IF_DESTROY_ENEMY" />
			<Set HPHealedIfDestroyEnemy="4"
				 />
		</Update>
		<Update>
			<Where Type="PROMOTION_FASTER_HEAL" />
			<Set EnemyHealChange="2"
				 NeutralHealChange="2"
				 FriendlyHealChange="4"
				 />
		</Update>
	</UnitPromotions>


	<!-- 
			Soldiers
	-->

	<UnitPromotions>
		<Update>
			<Where Type="PROMOTION_SIEGE" />
