				 PromotionPrereqOr1="PROMOTION_SHOCK_1"
				 PromotionPrereqOr2="PROMOTION_DRILL_1"
				 />
		</Update>
		<Update>
			<Where Type="PROMOTION_AMBUSH_2" />
